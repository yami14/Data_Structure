def search(arr, n, x)
  for i in range(0, n):
      if(arr[i]==x):
        return 1;
  return -1;
  
  arr=[2,4,5,7,80,96]
  x=96;
  n= len(arr);
  result= search(arr, n, x)
  if (result== -1):
      print("Element not found in the given list")
  else:
      print("Element found in the given list", result)      
